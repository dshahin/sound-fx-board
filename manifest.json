{
  "name": "Sound Effects Board",
  "version": "2.1",
  "manifest_version": 2,
  "description": "Personal Sound FX board for chrome",
  "browser_action": {
    "default_icon": "/img/Speaker-icon-19.png",
    "default_popup": "/html/popup.html",
    "default_title": "SFX"
  },
  "permissions": [
    "notifications"
  ],
  "web_accessible_resources": [
    "/img/icon.png"
  ]

}
